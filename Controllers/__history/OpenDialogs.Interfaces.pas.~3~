unit OpenDialogs.Interfaces;

interface

type
  TOpenDialogs = class(TInterfacedObject, iDialog)
    constructor Create;
    destructor Destroy; override;
    class function New : iDialog;
  end;

implementation

{ TOpenDialogs }

constructor TOpenDialogs.Create;
begin

end;

destructor TOpenDialogs.Destroy;
begin

  inherited;
end;

class function TOpenDialogs.New: iDialog;
begin
  Result := Self.Create;
end;

end.

unit Dialog.Controllers.Factory;

interface

uses
  Dialog.Controllers.Factory.Interfaces;

type
  TDialogFactory = class(TInterfacedObject, iDialogFactory)
  private
    FTypeDialog = TTypeDialogs;
  public
    function Dialogs(const TypeDialog: TTypeDialogs): iDialogFactory;
    constructor Create;
    destructor Destroy; override;
    class function New: iDialogFactory;
  end;

implementation

{ TDialogFactory }

constructor TDialogFactory.Create;
begin

end;

destructor TDialogFactory.Destroy;
begin

  inherited;
end;

function TDialogFactory.Dialogs(TypeDialog: TTypeDialogs): iDialogFactory;
begin

end;

class function TDialogFactory.New: iDialogFactory;
begin
  Result := Self.Create;
end;

end.
